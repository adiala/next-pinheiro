  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "dotenv": "^10.0.0",
    "next": "^11.0.0",
    "nodemailer": "^6.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "postcss": "^8.3.4",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.1.4"
  }
}
